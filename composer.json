{
    "name": "authentiq/oauth2-authentiq-php",
    "description": "Authentiq OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandros Keramidas",
            "email": "dev.alexkeramidas@gmail.com",
            "homepage": "https://github.com/alexkeramidas"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentiq"
    ],
    "require": {
        "php": ">=5.4.0",
        "league/oauth2-client": "^2.2",
        "firebase/php-jwt": "~3.0||~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": {
            "Authentiq\\OAuth2\\Client\\": "src/"
